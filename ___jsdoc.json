{
    "source": {
      "include": ["./"],
      "exclude": ["node_modules", "jsdoc"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./jsdoc/",
        "recurse": true,
        "verbose": true,
        "template": "templates/default"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true
    }
  }
  